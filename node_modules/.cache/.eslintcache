rd&apikey=db272c67\";\n\n\nconst App = () => {\n  const [state, dispatch] = useReducer(reducer, initialState);\n\n  useEffect(() => {\n    axios.get(MOVIE_API_URL).then(jsonResponse => {\n      dispatch({\n        type: \"SEARCH_MOVIES_SUCCESS\",\n        payload: jsonResponse.data.Search\n      });\n    });\n  }, []);\n\n  // you can add this to the onClick listener of the Header component\n  const refreshPage = () => {\n    window.location.reload();\n  };\n\n\n  const search = searchValue => {\n    dispatch({\n      type: \"SEARCH_MOVIES_REQUEST\"\n    });\n\n    axios(`http://www.omdbapi.com/?s=${searchValue}&apikey=db272c67`).then(\n      jsonResponse => {\n        if (jsonResponse.data.Response === \"True\") {\n          dispatch({\n            type: \"SEARCH_MOVIES_SUCCESS\",\n            payload: jsonResponse.data.Search\n          });\n        } else {\n          dispatch({\n            type: \"SEARCH_MOVIES_FAILURE\",\n            error: jsonResponse.data.Error\n          });\n        }\n      }\n    );\n  };\n\n  const { movies, errorMessage, loading } = state;\n\n  const retrievedMovies =\n    loading && !errorMessage ? (\n      <img className=\"loader\" src={loader} alt=\"Loading\" />\n    ) : errorMessage ? (\n      <div className=\"errorMessage\">{errorMessage}</div>\n    ) : (\n      movies.map((movie, index) => (\n        <Movie key={`${index}-${movie.Title}`} movie={movie} />\n      ))\n    );\n\n  return (\n    <div className=\"App\">\n      <div className=\"m-container\">\n        <Header text=\"Search Movies\" />\n        <Search search={search} />\n        <p className=\"App-Intro\"></p>\n        <div className=\"movies\">{retrievedMovies}</div>\n      </div>\n    </div>\n  );\n};\n\nexport default App;","/Users/university/Desktop/Portfolio/Movie Finder/movie_finder/src/components/Movie.js",[],"/Users/university/Desktop/Portfolio/Movie Finder/movie_finder/src/components/Header.js",[],"/Users/university/Desktop/Portfolio/Movie Finder/movie_finder